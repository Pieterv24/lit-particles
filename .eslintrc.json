{
  "extends": [
    "@open-wc/eslint-config",
    "eslint-config-prettier"
  ],
  "rules": {
    "lines-between-class-members": [
      "error", "always", {"exceptAfterSingleLine": true}
    ],
    "no-plusplus": "off",
    "no-else-return": ["error", {"allowElseIf": true}],
    "class-methods-use-this": "warn"
  }
}
